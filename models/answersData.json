{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://mobistudy.org/models/answersData.json",
  "title": "Answers",
  "description": "Detailed answers given to a form",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "timeStamp": {
        "type": "string"
      },
      "questionId": {
        "type": "string"
      },
      "questionType": {
        "type": "string"
      },
      "questionText": {
        "type": "object"
      },
      "answer": {
        "type": [
          "string",
          "number",
          "integer",
          "object",
          "array"
        ]
      }
    },
    "required": [
      "questionId"
    ]
  }
}
