{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://mobistudy.org/models/miband3Summary.json",
  "title": "Miband3 data download summary",
  "description": "Summary of the data downloaded from the MiBand3.",
  "type": "object",
  "properties": {
    "length": {
      "description": "Total number of samples retrieved.",
      "type": "number"
    },
    "firstTS": {
      "description": "timestamp of the first sample",
      "type": "string"
    },
    "lastTS": {
      "description": "timestamp of the last sample",
      "type": "string"
    }
  },
  "required": [
    "length",
    "firstTS",
    "lastTS"
  ]
}
